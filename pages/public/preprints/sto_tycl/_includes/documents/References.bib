@article{Agarwal2019,
    abstract = {The present work is devoted to study the asymptotic nature of a stochastic pine wilt disease model. Investigating the basic reproductive number, we proved that it has sharp threshold properties. The underlying system of stochastic differential equations is analyzed, and proper Lyapunov functionals are formulated to show the stability analysis. Adequate conditions are provided to study the extinction and permanence of the disease. Reducing infected individuals involved in a substance abuse is a challenging task therefore, using some control variables the stochastic optimality system, is obtained. Furthermore, for the authenticity of the obtained results numerical simulations are carried out at the end of the manuscript.},
    author = {Agarwal, Ravi P. and Badshah, Qaisar and ur Rahman, Ghaus and Islam, Saeed},
    doi = {10.1016/j.jfranklin.2019.03.007},
    file = {:C$\backslash$:/Users/adria/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Agarwal et al. - 2019 - Optimal control {\&} dynamical aspects of a stochastic pine wilt disease model.pdf:pdf},
    issn = {00160032},
    journal = {Journal of the Franklin Institute},
    month = {may},
    number = {7},
    pages = {3991--4025},
    publisher = {Elsevier Ltd},
    title = {{Optimal control \& dynamical aspects of a stochastic pine wilt disease model}},
    url = {https://doi.org/10.1016/j.jfranklin.2019.03.007 https://linkinghub.elsevier.com/retrieve/pii/S0016003219302029},
    volume = {356},
