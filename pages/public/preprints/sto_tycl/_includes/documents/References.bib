    issn = {0031-949X},
    journal = {Phytopathology},
    month = {aug},
    number = {10},
    pages = {PHYTO--03--17--008},
    title = {{Modeling Virus Coinfection to Inform Management of Maize Lethal Necrosis in Kenya}},
